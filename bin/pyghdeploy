#!/usr/bin/python

import cherrypy, json

class HelloWorld:
  def index(self):
    return "Hello world!"
  index.exposed = True

  def github(self, payload=None):
    data = json.loads(payload)
    message = json.dumps(data, sort_keys=True, indent=4) + "\n"
    message += data['repository']['name'] + "\nok.\n"
    return message
  github.exposed = True

cherrypy.quickstart(HelloWorld())

